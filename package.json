{
  "name": "dcs-lua-injector",
  "displayName": "dcs-lua-injector",
  "description": "Runs lua code in the mission environment",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "dcs-lua-injector.run",
      "title": "Run lua code in DCS"
    }],
    "keybindings":[
      {
        "command": "dcs-lua-injector.run",
        "key": "alt+d"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dcs-script-injector",
          "title": "DCS Script Injector",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "dcs-script-injector": [
        {
          "id": "dcs-injector-scripts",
          "name": "Scripts"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
